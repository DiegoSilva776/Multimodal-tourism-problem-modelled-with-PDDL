; estacao
; ciclista
; bicicleta
;
;
;
(define (domain turismo-multimodal-a)
  (:requirements :strips)
  (:predicates
   (at ?what ?estacao)
   (adjacente ?estacao-1 ?estacao-2)
   (bike ?estacao)
   (is-ciclista ?who)
   (is-gold ?what)
   (estacao-visitada ?estacao ?ciclista))

  (:action pedalar
    :parameters (?who ?from ?to)
    :precondition (and (is-ciclista ?who)
		       (at ?who ?from)
		       (adjacente ?from ?to))
    :effect (and (not (at ?who ?from))
		             (at ?who ?to)
                 (estacao-visitada ?to))
    )

  
)

